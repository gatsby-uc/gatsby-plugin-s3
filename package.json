{
    "name": "gatsby-plugin-s3",
    "version": "0.2.4",
    "description": "Enables you to deploy your gatsby site to a S3 bucket.",
    "main": "bin.js",
    "bin": "bin.js",
    "scripts": {
        "test": "jest",
        "prepublish": "npm run build",
        "build": "tsc --project .",
        "develop": "babel --extensions=\".ts\" --out-dir=. src/ -w"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jariz/gatsby-plugin-s3.git"
    },
    "keywords": [
        "gatsby",
        "gatsby-plugin",
        "aws",
        "s3",
        "aws-sdk"
    ],
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-typescript": "^7.1.0",
        "@types/fs-extra": "^5.0.4",
        "@types/inquirer": "0.0.43",
        "@types/klaw": "^2.1.1",
        "@types/mime": "^2.0.0",
        "@types/minimatch": "^3.0.3",
        "@types/node-fetch": "^2.1.6",
        "@types/ora": "^3.0.0",
        "@types/stream-to-promise": "^2.2.0",
        "@types/yargs": "^12.0.2",
        "gatsby": "^2.0.72",
        "node-fetch": "^2.3.0",
        "typescript": "^3.2.2"
    },
    "peerDependencies": {
        "gatsby": ">2.0.0-alpha"
    },
    "author": "Jari Zwarts <jarizw@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jariz/gatsby-plugin-s3/issues"
    },
    "homepage": "https://github.com/jariz/gatsby-plugin-s3#readme",
    "dependencies": {
        "@types/jest": "^24.0.0",
        "aws-sdk": "^2.382.0",
        "chalk": "^2.4.1",
        "fs-extra": "^7.0.1",
        "inquirer": "^6.2.1",
        "jest": "^23.6.0",
        "klaw": "^3.0.0",
        "mime": "^2.4.0",
        "minimatch": "^3.0.4",
        "ora": "^3.0.0",
        "pretty-error": "^2.1.1",
        "stream-progressbar": "^1.1.1",
        "stream-to-promise": "^2.2.0",
        "ts-jest": "^23.10.5",
        "yargs": "^12.0.5"
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
