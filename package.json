    "repository": {
        "type": "git",
        "url": "git+https://github.com/jariz/gatsby-plugin-s3.git"
    },
    "keywords": [
        "gatsby",
        "gatsby-plugin",
        "aws",
        "s3",
        "aws-sdk"
    ],
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-typescript": "^7.1.0",
        "@types/es6-promisify": "^6.0.0",
        "@types/fs-extra": "^5.0.4",
        "@types/glob": "^7.1.1",
        "@types/inquirer": "0.0.43",
        "@types/is-ci": "^1.1.0",
        "@types/klaw": "^2.1.1",
        "@types/mime": "^2.0.0",
        "@types/minimatch": "^3.0.3",
        "@types/node-fetch": "^2.1.6",
        "@types/ora": "^3.0.0",
        "@types/stream-to-promise": "^2.2.0",
        "@types/yargs": "^12.0.2",
        "es6-promisify": "^6.0.1",
        "gatsby": "^2.0.72",
        "glob": "^7.1.3",
        "node-fetch": "^2.3.0",
        "typescript": "^3.2.2"
    },
    "peerDependencies": {
