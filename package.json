    "author": "Jari Zwarts <jarizw@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jariz/gatsby-plugin-s3/issues"
    },
    "homepage": "https://github.com/jariz/gatsby-plugin-s3#readme",
    "dependencies": {
        "@types/jest": "^24.0.0",
        "aws-sdk": "^2.382.0",
        "chalk": "^2.4.1",
        "fs-extra": "^7.0.1",
        "fs-posix": "^6.0.0",
        "inquirer": "^6.2.1",
        "is-ci": "^2.0.0",
        "jest": "^23.6.0",
        "klaw": "^3.0.0",
        "mime": "^2.4.0",
        "minimatch": "^3.0.4",
        "ora": "^3.0.0",
        "pretty-error": "^2.1.1",
        "stream-progressbar": "^1.1.1",
        "stream-to-promise": "^2.2.0",
        "yargs": "^12.0.5"
    },
    "jest": {
        "roots": [
            "<rootDir>"
